put

scfdata= {
   ! scf_type=        gchf
   kind=            rhf
   ! scf_type=        uhf
   ! initial_density=   core
   initial_density=   promolecule
   ! initial_mos=     unrestricted
    direct=          on
    diis=           { 
!   diis_keep=       8
    start_iteration=      1
   ! converge=      0.000001
   ! diis_convergence= 0.000001
   convergence_tolerance=       0.0000001 }
!    use_bl_term=       no
    use_bs_term=       no
    use_bs_t_term=       no
    use_aa_term=       no
    use_1e_s(rxa)_term=  no
    use_1e_sl_term=    no
    use_2e_sl_term=    no
