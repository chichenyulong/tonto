{
   
   output_style_options= {
      real_precision= 4
   }

   read_g09_fchk_file karrikinolide.fchk

   name= karrikinolide

   put

   write_archive molecular_orbitals

   scfdata= {

      kind=            rks

      initial_MOs= restricted

      convergence= 0.001
      diis= { convergence_tolerance= 0.001 }

      output= NO
      output_results= YES

   }

   scf

   ! Roby population analysis
   robydata= {

      kind= atom_energy_bond_analysis

      atom_list= { 1 ... 17 }

      output_theta_info= NO
    ! analyze_all_atom_pairs= TRUE
    ! covalent_cutoff = 89 degree

   }

   roby_analysis

}
