{
   
   output_style_options= {
      real_precision= 4
   }

   read_g09_fchk_file karrikinolide.fchk

   name= karrikinolide

   put

   write_archive molecular_orbitals

   scfdata= {
      kind=            rks
      dft_exchange_functional= becke88
      dft_correlation_functional= lyp

      ! WARNING : need this option
      ! for correct atom projectors
      initial_density= promolecule

      convergence= 0.00001
      diis= { convergence_tolerance= 0.00001 }

      output= NO
      output_results= YES
   }

   scf

   ! Roby population analysis
   robydata= {
      kind= atom_bond_analysis
      output_theta_info= NO
      use_NAOs= YES
   }

   roby_analysis

}
